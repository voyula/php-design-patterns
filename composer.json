{
    "name": "voyula/php-design-patterns",
    "description": "PHP library for data format validation.",
    "keywords": [
        "validator"
    ],
    "homepage": "https://github.com/voyula/php-design-patterns",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "voyula",
            "homepage": "https://github.com/voyula",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Voyula\\Validate\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Voyula\\Validate\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs",
        "fix-style": "phpcbf"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
